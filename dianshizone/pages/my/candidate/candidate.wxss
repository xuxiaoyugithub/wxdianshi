/**index.wxss**/
page{
    width: 100%;
    height: 100%;
}
.container{
    width: 100%;
    height: 100%;
}
.nav {
    height: 80rpx;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    line-height: 80rpx;
    background: white;
    font-size: 16px;
    white-space: nowrap;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 99;
}
.nav-item {
    width: 20%;
    display: inline-block;
    text-align: center;
}
.nav-item.active{
    color: red;
}
.tab-box{
    width:100%;
    background:#F8F8F8;
    position:absolute;
    padding-top: 80rpx;
    top:0rpx;
    height: 100%;
    box-sizing: border-box;
}
.tab-content{
    overflow-y: scroll;
}
.user{
  width:95%;
  height:200rpx;
  background-color:white;
  display:flex;
  flex-direction: row;
  margin:auto;
  border-radius: 20rpx;
  margin-top:20rpx;
  text-align:center;
  align-items:center;
  /* justify-content: center; */
}
.user image{
  width:150rpx;
  height:150rpx;
  border-radius: 75rpx;
}
.dd{
  height:80rpx;
  position:absolute;
  right:30rpx;
  background-color:#F8F8F8;
  display:flex;
  text-align:center;
  align-items:center;
}
.completion{
  width:95%;
  height:90rpx;
  margin:auto;
  display:flex;
  align-items: center;
  justify-content: center;
  background-color:#ccc;
  line-height:40rpx;
}
.com{
  width:95%;
  display:flex;
  flex-direction: row;
  margin:auto;
}
.complete{
  width:50%;
  display:flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  background-color:orange;
}
.completes{
  width:50%;
  display:flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  background-color:orange;
}